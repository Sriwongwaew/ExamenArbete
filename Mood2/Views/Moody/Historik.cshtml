@model List<History>
@{
    Layout = "_LayoutMood";
}


<body>
    <div id="View1" class="container" style="margin-top: 50px;">

        <ul class="nav nav-tabs">
            <li class="nav-item">
                <a class="nav-link active" asp-action="Historik">Historik</a>
            </li>

        </ul>

        <table class="table table-striped table-dark">
            <thead>
                <tr>
                    <th scope="col">Datum</th>
                    <th scope="col">Namn</th>
                    <th scope="col">Mood</th>
                </tr>
            </thead>
            <tbody>
                @foreach (var item in Model)
                {
                <tr>
                    <td scope="row">@item.DateWhenPlayed</td>
                    <td>
                        <a href="@item.PlaylistLink">@item.PlaylistName</a>
                    </td>
                    <td>@item.Emotion</td>
                </tr>
                }
                

            </tbody>
        </table>


    </div>

    <link rel="stylesheet" href="~/css/main.css" />
</body>
